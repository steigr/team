_lib_var_name() {
  echo "_$(echo $1 | tr "[:lower:]" "[:upper:]" | sed -e 's:-:_:g')_LIB"
}

_lib_var() {
  n=$(_lib_var_name $1)
  echo ${!n}
}

_loaded() {
  export $(_lib_var_name $1)=true
}

_load() {
  if [[ "x$(_lib_var $1)" != "xtrue" ]]; then
    _loaded $1
    . $TEAM_LIB/$1.lib
  fi
}

require() {
  _load $1
}