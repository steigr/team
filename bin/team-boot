#!/bin/sh

. $TEAM_LIB/dependency.lib
require team

$TEAM ssh       rebind
$TEAM docker    bind tcp

$TEAM serf      restart

$TEAM bee       install
$TEAM swarm     install

$TEAM etcd      configure
$TEAM flanneld  configure

$TEAM interlock install
$TEAM registry  install

$TEAM zookeeper install

$TEAM dokku     install
$TEAM dokku     nerve
