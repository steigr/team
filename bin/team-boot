#!/bin/sh

. $TEAM_LIB/dependency.lib
require team

$TEAM ssh       rebind
$TEAM docker    bind tcp

$TEAM serf      install

$TEAM bee       install
$TEAM swarm     install

$TEAM etcd      install
$TEAM flanneld  install

$TEAM interlock install
$TEAM registry  install

$TEAM zookeeper install

$TEAM dokku     install
$TEAM dokku     nerve
